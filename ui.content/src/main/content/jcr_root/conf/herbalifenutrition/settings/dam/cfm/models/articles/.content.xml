<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Template"
    allowedPaths="[/content/entities(/.*)?]"
    ranking="{Long}100">
    <jcr:content
        cq:lastModified="{Date}2019-10-11T13:12:03.953-05:00"
        cq:lastModifiedBy="admin"
        cq:scaffolding="/conf/herbalifenutrition/settings/dam/cfm/models/articles/jcr:content/model"
        cq:templateType="/libs/settings/dam/cfm/model-types/fragment"
        jcr:primaryType="cq:PageContent"
        jcr:title="Article"
        sling:resourceSuperType="dam/cfm/models/console/components/data/entity"
        sling:resourceType="dam/cfm/models/console/components/data/entity/default">
        <model
            cq:targetPath="/content/entities"
            jcr:primaryType="cq:PageContent"
            sling:resourceType="wcm/scaffolding/components/scaffolding"
            dataTypesConfig="/mnt/overlay/settings/dam/cfm/models/formbuilderconfig/datatypes"
            maxGeneratedOrder="20">
            <cq:dialog
                jcr:primaryType="nt:unstructured"
                sling:resourceType="cq/gui/components/authoring/dialog">
                <content
                    jcr:lastModified="{Date}2019-10-11T13:12:03.953-05:00"
                    jcr:lastModifiedBy="admin"
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                    <items
                        jcr:primaryType="nt:unstructured"
                        maxGeneratedOrder="20">
                        <title-text
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                            fieldLabel="Title"
                            listOrder="2"
                            maxlength="255"
                            metaType="text-single"
                            name="title-text"
                            renderReadOnly="false"
                            required="on"
                            showEmptyInReadOnly="true"
                            valueType="string"/>
                        <title-text-link
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="dam/cfm/models/editor/components/contentreference"
                            fieldLabel="Title Link"
                            filter="hierarchy"
                            listOrder="3"
                            metaType="reference"
                            name="title-text-link"
                            nameSuffix="contentReference"
                            renderReadOnly="false"
                            showEmptyInReadOnly="true"
                            valueType="string"/>
                        <article-image
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="dam/cfm/models/editor/components/contentreference"
                            fieldLabel="Image"
                            filter="hierarchy"
                            listOrder="4"
                            metaType="reference"
                            name="article-image"
                            nameSuffix="contentReference"
                            renderReadOnly="false"
                            required="on"
                            rootPath="/content/dam/herbalife/consumable_content"
                            showEmptyInReadOnly="true"
                            valueType="string"/>
                        <date-published
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/form/datepicker"
                            displayedFormat="YYYY-MM-DD"
                            emptyText="YYYY-MM-DD"
                            fieldLabel="Date Published"
                            listOrder="5"
                            maxDate="today"
                            metaType="date"
                            name="date-published"
                            renderReadOnly="false"
                            required="on"
                            showEmptyInReadOnly="true"
                            type="date"
                            valueFormat="YYYY-MM-DD[T]HH:mm:ss.000Z"
                            valueType="calendar">
                            <granite:data
                                jcr:primaryType="nt:unstructured"
                                typeHint="Date"/>
                        </date-published>
                        <article-author
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                            fieldLabel="Author"
                            listOrder="6"
                            maxlength="255"
                            metaType="text-single"
                            name="article-author"
                            renderReadOnly="false"
                            showEmptyInReadOnly="true"
                            valueType="string"/>
                        <article-description
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="dam/cfm/admin/components/authoring/contenteditor/multieditor"
                            cfm-element="Short Description"
                            default-mime-type="text/html"
                            listOrder="7"
                            metaType="text-multi"
                            name="article-description"
                            renderReadOnly="false"
                            showEmptyInReadOnly="true"
                            valueType="string"/>
                        <article-content
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="dam/cfm/admin/components/authoring/contenteditor/multieditor"
                            cfm-element="Content"
                            default-mime-type="text/html"
                            listOrder="8"
                            metaType="text-multi"
                            name="article-content"
                            renderReadOnly="false"
                            showEmptyInReadOnly="true"
                            valueType="string"/>
                        <readmore-text-link
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="dam/cfm/models/editor/components/contentreference"
                            fieldLabel="Read More Link"
                            filter="hierarchy"
                            listOrder="9"
                            metaType="reference"
                            name="readmore-text-link"
                            nameSuffix="contentReference"
                            renderReadOnly="false"
                            showEmptyInReadOnly="true"
                            valueType="string"/>
                    </items>
                </content>
            </cq:dialog>
        </model>
